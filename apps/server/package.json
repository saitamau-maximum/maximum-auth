{
  "name": "maximum-auth-server",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev src/index.ts",
    "deploy": "wrangler deploy",
    "build": "esbuild src/index.ts --tsconfig=tsconfig.build.json",
    "test": "vitest",
    "db:migrations:gen": "drizzle-kit generate:sqlite",
    "db:migrations:apply:local": "wrangler d1 migrations apply maximum-members-db --local",
    "db:migrations:apply": "wrangler d1 migrations apply maximum-members-db"
  },
  "module": "src/index.ts",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "dependencies": {
    "@hono/valibot-validator": "^0.2.1",
    "drizzle-orm": "^0.29.1",
    "hono": "^3.11.6",
    "valibot": "^0.22.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230914.0",
    "drizzle-kit": "^0.20.6",
    "esbuild": "^0.19.9",
    "wrangler": "^3.15.0"
  }
}
